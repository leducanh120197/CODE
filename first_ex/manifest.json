{
  "name": "AnhLD1 Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {

  },
  "background" : {
    "scripts" : ["background.js"]
 },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}